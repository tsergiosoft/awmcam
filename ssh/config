Host github.com
	User git
	Port 22
	Hostname github.com
	IdentityFile ~/.ssh/id_ed25519
	TCPKeepAlive yes
	IdentitiesOnly yes

Host 31.116.134.20
	User tunkey
	IdentityFile ~/.ssh/tunkey
	TCPKeepAlive yes
	IdentitiesOnly yes
	GatewayPorts yes
	RemoteForward 14455 localhost:14455
	RemoteForward 8080 localhost:8080
